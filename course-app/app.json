{
  "expo": {
    "name": "course-app",
    "slug": "course-app",
    "scheme": "courseapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/apple-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/apple-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.phatbee.courseapp",
      "versionCode": 1,
      "version": "1.0.0",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/icons/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-secure-store",
      "expo-web-browser",
      "expo-video"
    ],
    "extra": {
      "eas": {
        "projectId": "8f990215-9aa2-4f60-895c-a82d64e0eb34"
      }
    }
  }
}
